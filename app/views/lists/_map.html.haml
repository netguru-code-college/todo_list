:javascript
  handler = Gmaps.build('Google');
  handler.buildMap(
    {
      provider: {
        center: new google.maps.LatLng(50.0636153, 19.9317813),
        zoom: 18
      },
      internal: {
        id: 'map'
      }
    },
    function(){
      // handler.addMarkers(#{raw @lists.to_json});
      handler.addCircles(
        [{ lat: 50.0636153, lng: 19.9317813, radius: 5}],
        { strokeColor: '#3d501f' }
      );
      handler.addMarkers([
  	    {
  	      'lat': 50.0636153,
  	      'lng': 19.9317813,
  	      'picture': {
  	        'url': 'http://www.tamu.edu/faculty/ccbn/dewitt/adp/resources/images/shamrock.gif',
  	        'width':  36,
  	        'height': 36
  	      }
  	    }
   	]);
    }
  );
